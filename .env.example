DATABASE_URL="postgresql://postgres:postgres@localhost:5432/fastify-db?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
PORT=3000
NODE_ENV=development

# Rate Limiting
TELEMETRY_RATE_LIMIT=10  # requests per minute
RELAY_RATE_LIMIT=10       # requests per minute

# Mock Provider Settings
MOCK_PAYMENT_FAILURE_RATE=0.3  # 30% failure rate for testing
MOCK_RELAY_FAILURE_RATE=0.2    # 20% failure rate for testing

# Logging
LOG_LEVEL=i